{
  "name": "02-api-rest-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "lint": "eslint src --ext .ts --fix",
    "knex": "node --no-warnings --loader tsx ./node_modules/knex/bin/cli.js",
    "migrate:create": "npm run knex -- migrate:make --migrations-directory ./db/migrations -x ts",
    "migrate:latest": "npm run knex -- migrate:latest --knexfile ./knexfile.ts",
    "migrate:rollback": "npm run knex -- migrate:rollback --knexfile ./knexfile.ts --all",
    "test": "vitest",
    "build": "tsup src --out-dir build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.2.0",
    "@types/node": "^18.14.2",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.35.0",
    "supertest": "^6.3.3",
    "tsup": "^6.7.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vitest": "^0.32.0"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.13.0",
    "knex": "^2.4.2",
    "pg": "^8.11.0",
    "sqlite3": "^5.1.6",
    "zod": "^3.21.4"
  }
}